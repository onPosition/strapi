{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Посты",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "post_category": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::posts-category.posts-category",
      "inversedBy": "posts"
    },
    "text": {
      "type": "blocks"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "dynamic_zone": {
      "type": "dynamiczone",
      "components": [
        "dinamyc-zone.cta",
        "dinamyc-zone.related-posts"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
